<!-- Content page -->
<section class="bgwhite p-t-55 p-b-65">
    <div class="container">
        <div class="row">
            <div class="col-sm-6 col-md-4 col-lg-3 p-b-50">
                <div class="leftbar p-r-20 p-r-0-sm">
                    <!--  -->
                    <h4 class="m-text14 p-b-7">
                        Categories
                    </h4>

                    <ul class="p-b-54">
                        <li class="p-t-4">
                            <a href="/product" class="s-text13 active1">
                               Show All
                            </a>
                        </li>

                        <li class="p-t-4">
                            <a href="/product?category=wannaone" class="s-text13">
                                Wanna one
                            </a>
                        </li>
                        <li class="p-t-4">
                            <a href="/product?category=nuest" class="s-text13">
                                Nu'est W
                            </a>
                        </li>
                        <li class="p-t-4">
                            <a href="/product?category=twice" class="s-text13">
                                Twice
                            </a>
                        </li>
                        <li class="p-t-4">
                            <a href="/product?category=gfriend" class="s-text13">
                               GFRIEND
                            </a>
                        </li>
                        <li class="p-t-4">
                            <a href="/product?category=momoland" class="s-text13">
                                MOMOLAND
                            </a>
                        </li>
                        <li class="p-t-4">
                            <a href="/product?category=cheongha" class="s-text13">
                                CHEONGHA
                            </a>
                        </li>

                    </ul>


                    <div class="search-product pos-relative bo4 of-hidden">
                        <input class="s-text7 size6 p-l-23 p-r-50" type="text" name="search-product" placeholder="Search Products...">

                        <button class="flex-c-m size5 ab-r-m color2 color0-hov trans-0-4">
                            <i class="fs-12 fa fa-search" aria-hidden="true"></i>
                        </button>
                    </div>
                </div>
            </div>

            <div class="col-sm-6 col-md-8 col-lg-9 p-b-50">
                <!--  -->
                <div class="flex-sb-m flex-w p-b-35">
                    <div class="flex-w">
                        <div class="rs2-select2 bo4 of-hidden w-size12 m-t-5 m-b-5 m-r-10">
                            <select class="selection-2" name="sorting">
                                <option>Default Sorting</option>
                                <option>Popularity</option>
                                <option>Price: low to high</option>
                                <option>Price: high to low</option>
                            </select>
                        </div>

                        <div class="rs2-select2 bo4 of-hidden w-size12 m-t-5 m-b-5 m-r-10">
                            <select class="selection-2" name="sorting">
                                <option>Price</option>
                                <option>$0.00 - $10.00</option>
                                <option>$10.00 - $30.00</option>
                                <option>$30.00 - $50.00</option>
                                <option>$50.00 - $70.00</option>
                                <option>$70.00+</option>

                            </select>
                        </div>
                    </div>

                    <span class="s-text8 p-t-5 p-b-5">
							Showing 1–12 of 12 results
						</span>
                </div>
                <!--
                <div class="block2-img wrap-pic-w of-hidden pos-relative block2-labelnew">
                <div class="block2-img wrap-pic-w of-hidden pos-relative">
                <div class="block2-img wrap-pic-w of-hidden pos-relative block2-labelsale">
                -->
                <!-- Product -->
                <div class="row">
                    <%
                    var loopcnt = 0;
                    var i=itemcount-1-pagecount*9
                    while(true){
                        if(loopcnt>8 || i<0) break;
                        if(goods[i].pd_viewPd) {
                            if(category=="wannaone" && goods[i].pd_category1 !="wannaone"){
                                i--;
                                continue;
                            }
                            if(category=="nuest" && goods[i].pd_category1 !="nuest"){
                                i--;
                                continue;
                            }
                            if(category=="twice" && goods[i].pd_category1 !="twice"){
                                i--;
                                continue;
                            }
                            if(category=="gfriend" && goods[i].pd_category1 !="gfriend"){
                                i--;
                                continue;
                            }
                            if(category=="momoland" && goods[i].pd_category1 !="momoland"){
                                i--;
                                continue;
                            }
                            if(category=="cheongha" && goods[i].pd_category1 !="cheongha"){
                                i--;
                                continue;
                            }
                    %>
                    <div class="col-sm-12 col-md-6 col-lg-4 p-b-50">
                        <!-- Block2 -->
                        <div class="block2">
                            <%if(goods[i].pd_viewNew){%>
                            <div class="block2-img wrap-pic-w of-hidden pos-relative block2-labelnew">
                                <%}
                            else if(goods[i].pd_viewSale){%>
                            <div class="block2-img wrap-pic-w of-hidden pos-relative block2-labelsale">
                                <%}
                            else{%>
                            <div class="block2-img wrap-pic-w of-hidden pos-relative">
                                <%}%>
                                <img src="/getimage/<%=goods[i].pd_id%>/1" alt="IMG-PRODUCT">

                                <div class="block2-overlay trans-0-4">
                                    <div class="block2-btn-addcart w-size1 trans-0-4">
                                        <!-- Button -->
                                        <button class="flex-c-m size1 bg4 bo-rad-23 hov1 s-text1 trans-0-4">
                                            Add to Cart
                                        </button>
                                    </div>
                                </div>
                            </div>

                            <div class="block2-txt p-t-20">
                                <a href="/product-detail?pd_id=<%=goods[i].pd_id%>" class="block2-name dis-block s-text3 p-b-5"><%=goods[i].pd_name%></a>
                                <a style="visibility: hidden" class="block2-id dis-block s-text3 p-b-5"><%=goods[i].pd_id%></a>
                                <span class="block2-price m-text6 p-r-5">
										$<%=goods[i].pd_price%>
									</span>
                            </div>
                        </div>
                    </div>
                    <%
                            loopcnt++;
                            i--;
                        } else{
                            i--;
                    }
                    }%>
                </div>

                <!-- Pagination -->
                <div class="pagination flex-m flex-w p-t-26">
                    <%for(var i=0;i<totalcount;i++){
                        if(category=='all' || category==''){
                            if(i==pagecount){ %>
                    <a href="/product?page=<%=i+1%>" class="item-pagination flex-c-m trans-0-4 active-pagination"><%=i+1%></a>
                    <%}
                    else{%>
                    <a href="/product?page=<%=i+1%>" class="item-pagination flex-c-m trans-0-4"><%=i+1%></a>
                        <%}
                        } else{
                            if(i==pagecount){ %>
                    <a href="/product?category=<%=category%>&page=<%=i+1%>" class="item-pagination flex-c-m trans-0-4 active-pagination"><%=i+1%></a>
                         <%}
                            else{%>
                    <a href="/product?category=<%=category%>&page=<%=i+1%>" class="item-pagination flex-c-m trans-0-4"><%=i+1%></a>
                            <%}
                            }
                    }%>
                </div>
            </div>
        </div>
    </div>
</section>



