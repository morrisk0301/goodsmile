            <!-- Header Icon -->
            <div class="header-icons">
                <li>
                    <img src="images/icons/icon-header-01.png" class="header-icon1" alt="ICON">
                    <ul class="sub_menu">
                        <% if (login_success) { %>
                        <% if (user.auth == 0){ %>
                        <li><a href="/administrator">Administrator</a></li>
                        <% } %>
                        <li><a>Welcome <%= user.name %>!</a></li>
                        <li></li>
                        <li><a href="/myorder">My Order</a></li>
                        <li><a href="/profile">View Profile</a></li>
                        <li><a href="/logout">Sign Out</a></li>
                        <% } else { %>
                        <li><a href="/login">Sign In</a></li>
                        <li><a href="/signup">Sign Up</a></li>
                        <li><a href="/auth/facebook" class="btn btn-primary"><span class="fa fa-facebook"></span> Sign In with Facebook</a></li>
                        <li><a href="/auth/twitter" class="btn btn-primary"><span class="fa fa-twitter"></span> Sign In with Twitter</a></li>
                        <% } %>
                    </ul>
                </li>

                <span class="linedivide1"></span>

                <div class="header-wrapicon2">
                    <img src="images/icons/icon-header-02.png" class="header-icon1 js-show-header-dropdown" alt="ICON">
                    <% if (login_success) { %>
                    <span class="header-icons-noti"><%=cart.length%></span>

                    <!-- Header cart noti -->
                    <div class="header-cart header-dropdown">
                        <ul class="header-cart-wrapitem">
                            <%
                                var total = 0;
                            for(var i=0;i<cart.length;i++){
                            %>
                            <li class="header-cart-item">
                                <div class="header-cart-item-img">
                                    <img src="/getimage/<%=cart[i].cart_id%>/1" alt="IMG">
                                </div>

                                <div class="header-cart-item-txt">
                                    <a href="/product-detail?pd_id=<%=cart[i].cart_id%>" class="header-cart-item-name"><%=cart[i].cart_name%></a>
                                    <a style="visibility: hidden" class="header-cart-item-id"><%=cart[i].cart_id%></a>

                                    <span class="header-cart-item-info">
											<%=cart[i].cart_num%> x $<%=cart[i].cart_price%>
										</span>
                                </div>
                            </li>
                            <%
                                total += cart[i].cart_num * cart[i].cart_price;
                            }%>
                        </ul>

                        <div class="header-cart-total">
                            Total: $<%=Math.round((total) * 100) / 100%>
                        </div>

                        <div class="header-cart-buttons">
                            <div class="header-cart-wrapbtn">
                                <!-- Button -->
                                <a href="/cart" class="flex-c-m size1 bg1 bo-rad-20 hov1 s-text1 trans-0-4">
