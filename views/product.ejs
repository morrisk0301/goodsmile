<script type="text/javascript">
    $('.header-cart-wrapbtn').on('click', function(){
        <%if(login_success){
        for(var i=0;i<cart.length;i++){%>
        $.post("/addorder", {
            pd_id: "<%=cart[i].cart_id%>",
            pd_name: "<%=cart[i].cart_name%>",
            pd_num: parseInt("<%=cart[i].cart_num%>"),
            pd_price: parseFloat("<%=cart[i].cart_price%>"),
            pd_weight: parseFloat("<%=cart[i].cart_weight%>"),
            isfirst: parseInt("<%=i==0?1:0%>")
        }, function(data, status){});
        <%}
        }%>
        window.location='/order'
    });
    $('.header-cart-wrapbtn-mbile').on('click', function(){
        <%if(login_success){
        for(var i=0;i<cart.length;i++){%>
        $.post("/addorder", {
            pd_id: "<%=cart[i].cart_id%>",
            pd_name: "<%=cart[i].cart_name%>",
            pd_num: parseInt("<%=cart[i].cart_num%>"),
            pd_price: parseFloat("<%=cart[i].cart_price%>"),
            pd_weight: parseFloat("<%=cart[i].cart_weight%>"),
            isfirst: parseInt("<%=i==0?1:0%>")
        }, function(data, status){});
        <%}
        }%>
        window.location='/order'
    });
    $('.header-cart-item-img').each(function(){
        var idCart = $(this).parent().find('.header-cart-item-id').html();
        var nameCart = $(this).parent().find('.header-cart-item-name').html();
        $(this).on('click', function(){
            $.get("/delcart?pd_id="+idCart, function(data, status){
            });
            swal({
                title:nameCart,
                text:"is removed from cart",
                icon:"success",
                buttons:"OK"
            }).then(function(){
                location.reload();
            });
        });
    });
</script>
<script type="text/javascript">
    /*[ No ui ]
    ===========================================================*/
    var filterBar = document.getElementById('filter-bar');

    noUiSlider.create(filterBar, {
        start: [ 50, 200 ],
        connect: true,
        range: {
            'min': 50,
            'max': 200
        }
    });

    var skipValues = [
        document.getElementById('value-lower'),
        document.getElementById('value-upper')
    ];

    filterBar.noUiSlider.on('update', function( values, handle ) {
        skipValues[handle].innerHTML = Math.round(values[handle]) ;
    });
</script>
